// Method 1 (Using Extra Space)
import java.util.Scanner;
public class Test
{
       public static void main(String[] args)
       {
         Scanner sc=new Scanner(System.in);
         System.out.println("Enter the Size of the Array");
         int n=sc.nextInt();
         if(n>0)
         {
           int[] arr=new int[n];
           System.out.printf("Enter %d Elements of Array:\n",n);
           for(int i=0;i<n;i++)
              arr[i]=sc.nextInt();
	      
           int newLen=removeDuplicatesFromSortedArray(arr);
	   
           System.out.println("Duplicate Free Array is :");
           for(int i=0;i<newLen;i++)
              System.out.printf(arr[i]+" ");
         }
         else
            System.out.println("Invalid Choice");
	}
	
        public static int removeDuplicatesFromSortedArray(int[] arr)
	{
           int n=arr.length,j=0;
           int[] temp=new int [n];
	   
	   for(int i=0;i<(n-1);i++)
             if(arr[i]!=arr[i+1])
               temp[j++]=arr[i];
              
           temp[j++]=arr[n-1];
       
           for(int i=0;i<j;i++)
             arr[i]=temp[i];
	     
	   return j;
	}
}


// Method 2 (Using Extra Space)
import java.util.*;
public class Test
{
      public static void main(String[] args)
      {
         Scanner sc=new Scanner(System.in);
         System.out.println("Enter the Size of the Array");
         int n=sc.nextInt();
         if(n>0)
         {
           int[] arr=new int[n];
           System.out.printf("Enter %d Elements of Array:\n",n);
           for(int i=0;i<n;i++)
              arr[i]=sc.nextInt();
           int newLen=removeDuplicatesFromSortedArray(arr);
           System.out.println("Duplicate Free Array is :");
           for(int i=0;i<newLen;i++)
              System.out.printf(arr[i]+" ");
          }
          else
            System.out.println("Invalid Choice");
	}
	
        public static int  removeDuplicatesFromSortedArray(int[] arr)
	{
          int j=0;
          HashSet<Integer>hs=new HashSet<>();
	  
          for(int val:arr)
            hs.add(val);
      
          for(int val :arr)
            if(hs.contains(val))
            {
              arr[j++]=val;
              hs.remove(val);
            }
	    
          return j;
	}
}

// Method 3 (Using Extra Space)
import java.util.*;
public class Test
{
      public static void main(String[] args)
      {
         Scanner sc=new Scanner(System.in);
         System.out.println("Enter the Size of the Array");
         int n=sc.nextInt();
         if(n>0)
         {
           int[] arr=new int[n];
           System.out.printf("Enter %d Elements of Array:\n",n);
           for(int i=0;i<n;i++)
              arr[i]=sc.nextInt();
	      
           int newLen=removeDuplicatesFromSortedArray(arr);
	   
           System.out.println("Duplicate Free Array is :");
           for(int i=0;i<newLen;i++)
              System.out.printf(arr[i]+" ");
          }
          else
            System.out.println("Invalid Choice");
      }
	
      public static int  removeDuplicatesFromSortedArray(int[] arr)
      {
          int j=0;
          LinkedHashSet<Integer>lhs=new LinkedHashSet<>();
	  
          for(int val:arr)
            lhs.add(val);
      
          for(int key :lhs)
            arr[j++]=key;
    
          return j;  
      }
}


// Method 4 (Using Extra Space)
import java.util.*;
public class Test
{
      public static void main(String[] args)
      {
         Scanner sc=new Scanner(System.in);
         System.out.println("Enter the Size of the Array");
         int n=sc.nextInt();
         if(n>0)
         {
           int[] arr=new int[n];
           System.out.printf("Enter %d Elements of Array:\n",n);
           for(int i=0;i<n;i++)
              arr[i]=sc.nextInt();
	      
           int newLen=removeDuplicatesFromSortedArray(arr);
	   
           System.out.println("Duplicate Free Array is :");
           for(int i=0;i<newLen;i++)
              System.out.printf(arr[i]+" ");
          }
          else
            System.out.println("Invalid Choice");
      }
	
      public static int  removeDuplicatesFromSortedArray(int[] arr)
      {
          int j=0;
          TreeSet<Integer>ts=new TreeSet<>();
	  
          for(int val:arr)
            ts.add(val);
      
         
          for(int val :arr)
            if(ts.contains(val))
            {
              arr[j++]=val;
              ts.remove(val);
            }
           return j;
      }
}



//Method 5 (Without Using Extra Space)
import java.util.Scanner;
public class Test
{
      public static void main(String[] args)
      {
         Scanner sc=new Scanner(System.in);
         System.out.println("Enter the Size of the Array");
         int n=sc.nextInt();
         if(n>0)
         {
           int[] arr=new int[n];
           System.out.printf("Enter %d Elements of Array:\n",n);
           for(int i=0;i<n;i++)
              arr[i]=sc.nextInt();
           int newLen=removeDuplicatesFromSortedArray(arr);
           System.out.println("Duplicate Free Array is :");
           for(int i=0;i<newLen;i++)
              System.out.printf(arr[i]+" ");
          }
          else
             System.out.println("Invalid Choice");
	}
	
        public static int  removeDuplicatesFromSortedArray(int[] arr)
	{
           int n=arr.length,j=0;
	   for(int i=0;i<(n-1);i++)
             if(arr[i]!=arr[i+1])
               arr[j++]=arr[i];
	       
           arr[j++]=arr[n-1];
	   
	   return j;
	}
}
