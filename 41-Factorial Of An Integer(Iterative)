import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		System.out.println(factorial(num));
	}
	
	public static long factorial(int num)
	{
	    long fact=1;
	   for(int i=num;i>0;i--)
	      fact=fact*i;
	   return fact;
	}
}
