import java.util.Scanner;
import java.lang.Math;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num1=sc.nextInt();
		int num2=sc.nextInt();
		int num3=sc.nextInt();
		int res=Greatest1(num1,num2,num3);
		if(res==-1)
		   System.out.println("Numbers are Same");
		else 
		    System.out.println("Greatest Number is :"+res);
		    
		    System.out.println("Greatest Number is :"+Greatest2(num1,num2,num3));
	}
  
	//Method 1
	public static int Greatest1(int num1,int num2,int num3)
	{
	    if (num1 > num2 && num1 > num3)  
              return num1;  
        else if (num2 > num1 && num2 > num3)  
            return num2;  
        else if (num3 > num1 && num3 > num2)  
            return num3;  
        else  
            return -1;//numbers are same    
	}
	
	//Method 2
	public static int Greatest2(int num1,int num2,int num3)
	{
	    int temp=Math.max(num1,num2);
	    int max=Math.max(temp,num3);
	    return max;
	}
}
