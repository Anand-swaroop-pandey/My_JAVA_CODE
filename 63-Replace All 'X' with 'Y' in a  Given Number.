import java.util.Scanner;
public class Main
{
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		int x=sc.nextInt();
		int y=sc.nextInt();
		int num=sc.nextInt();
		System.out.println(replace(x,y,num));
		
	    System.out.println(reverseModified(num,0,x,y));
	}
    
	//Optimal Method
	public static int replace(int x,int y,int num)
	{
	    int sum=0;
	    int placeValue=1;
	    while(num!=0)
	    {
	        int rem=num%10;
	        if(rem==x)
               rem=y;
	       sum=sum+placeValue*rem;
	       placeValue=placeValue*10;
	       num=num/10;
	    }
	   return sum;  
	}
	
	//Method 2(Not efficient)
	 public static int reverseModified(int num,int rev,int x,int y)
    {
        if(num==0)
          return reverse(rev,0);
        else
           {
               int rem=num%10;
               if(rem==x)
                 rem=y;
               return reverseModified(num/10,rev*10+rem,x,y);
           }
    }
    
    public static int reverse(int num,int rev)
    {
        if(num==0)
          return rev;
        else
          return reverse(num/10,rev*10+(num%10));
    }
}
