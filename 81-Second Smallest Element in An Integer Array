import java.util.*;
public class Test
{
	public static void main(String[] args)
	{
		System.out.println("Enter the Size of the Array:");
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int [n];
		System.out.printf("Ente %d Elements\n",n);
		for(int i=0;i<arr.length;i++)
		  arr[i]=sc.nextInt();
		  sc.close();
		  System.out.println(secSmallestElement(arr));
		  System.out.println(secSmallestElement1(arr));
	}
	public static int secSmallestElement(int arr[])
	{
	    int min=Integer. MAX_VALUE;
	    int secMin=Integer. MAX_VALUE-1;
	    for(int i=0;i<arr.length;i++)
	         if(arr[i]<min)
	         {
	             secMin=min;
	             min=arr[i];
	         }
             else if((arr[i]>min) && (arr[i]<secMin))
                 secMin=arr[i];
	    if(secMin==(Integer. MAX_VALUE-1))
	    	return -1;
	    return secMin;      
	}
	
	public static int secSmallestElement1(int arr[])
	{
	    if((arr.length)<2)
	      return -1;
	    Arrays.sort(arr);
	    return arr[1];      
	}
}
