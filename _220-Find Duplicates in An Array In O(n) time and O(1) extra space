//Bruteforce Approach
import java.util.Scanner;
public class Test
{
	public static void main(String[] args) {
	   Scanner sc=new Scanner(System.in);
	   System.out.println("Enter the size of the array");
	   int n=sc.nextInt();
	   if(n>1)
	   {
	     int[] arr=new int[n];
	     System.out.printf("Enter %d elements of the array\n",n);
	     for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
	       
	     System.out.println("Duplicates Elements are:");
	     printDuplicate(arr);
	   }
	   else
	     System.out.println("Invalid Choice/No Duplicate");
	}
	
	
	public static void printDuplicate(int [] arr)
	{
	    int n=arr.length;
	    int[] freqArr=new int[n];
	    boolean flag=false;
	    
            countFrequecy(arr,freqArr);
         
            for(int i=0;i<n;i++)
              if(freqArr[i]>=2)
              {
                 System.out.println(arr[i]);
                 flag=true;
              }
           
            if(flag==false) 
               System.out.println("No Duplicates Elements Exists");
	}
	
	
	public static void countFrequecy(int[] arr,int[] freqArr)
	{
	   int n=arr.length,ount;   
           for(int i=0;i<n;i++)
           {
             if(freqArr[i]==-1)
               continue;
	       
               count=1;
             for(int j=i+1;j<n;j++)
                if(arr[i]==arr[j])
                {
                   freqArr[j]=-1;
                   count++;
                }
            freqArr[i]=count;       
           }
	}
}

//Improved Approach
import java.util.Scanner;
public class Test
{
	public static void main(String[] args) {
	   Scanner sc=new Scanner(System.in);
	   System.out.println("Enter the size of the array");
	   int n=sc.nextInt();
	   if(n>1)
	   {
	     int[] arr=new int[n];
	     System.out.printf("Enter %d elements of the array\n",n);
	     for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
	       
	     System.out.println("Duplicate Elements are:");
	     printDuplicate(arr);
	   }
	   else
	     System.out.println("Invalid Choice/No Duplicate");
	}
	
        public static void printDuplicate(int [] arr)
	{
	    int n=arr.length;
	    boolean flag=false;
            int[] freqArr=new int[n];
	    
            for(int i=0;i<n;i++)
              freqArr[arr[i]]++;
	      
            for(int i=0;i<n;i++)
              if(freqArr[i]>=2)
              {
                System.out.println(i);
                flag=true;
              }
	      
            if(flag==false)
              System.out.println("No Duplicate Element Exists:");
	}
}

//Better Approach
import java.util.*;
public class Test
{
    public static void main(String[] args)
    {	
           Scanner sc=new Scanner(System.in);
	   System.out.println("Enter the size of the array");
	   int n=sc.nextInt();
	   if(n>1)
	   {
	     int[] arr=new int[n];
	     System.out.printf("Enter %d elements of the array\n",n);
	     for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
	     
	     System.out.println("Duplicates Elements are:");
	     printDuplicate(arr);
	   }
	   else
	     System.out.println("Invalid Choice/No Duplicate");   
    }
		
    public static void printDuplicate(int arr[])
    {
       int n=arr.length;
       boolean flag=false;
       TreeMap<Integer,Integer>tm=new TreeMap<>();
 
       for(int val :arr)
         if(tm.containsKey(val))
           tm.put(val,tm.get(val)+1);
         else
           tm.put(val,1);
       
       for(int key : tm.keySet())
         if(tm.get(key)>1)
         {
           System.out.println(key);
           flag=true;
         }
           
       if(flag==false)
          System.out.println("No Duplicates Elements Exists");
    }
}

//Optimal Approach

import java.util.Scanner;
public class Test
{
	public static void main(String[] args) {
	   Scanner sc=new Scanner(System.in);
	   System.out.println("Enter the size of the array");
	   int n=sc.nextInt();
	   if(n>1)
	   {
	     int[] arr=new int[n];
	     System.out.printf("Enter %d elements of the array\n",n);
	     for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
	       
	     System.out.println("Duplicates Elements are:");
	     printDuplicate(arr);
	   }
	   else
	     System.out.println("Invalid Choice/No Duplicate");
	}
	
	public static void printDuplicate(int [] arr)
	{
	    boolean flag=false;
	    int n=arr.length,OrgValAt_ithIndex,countOf_i;
	    
	    for(int i=0;i<n;i++)
	    {
	        OrgValAt_ithIndex=arr[i]%n;
	        arr[OrgValAt_ithIndex]+=n;
	    }
	    
	    for(int i=0;i<n;i++)
	    {
	       countOf_i=(arr[i]/n);
	       
	       if(countOf_i>=2)
	       {
	         System.out.println(i);
	         flag=true;
	       }
	    }
	        
	    if(flag==false)
	      System.out.println("No Duplicates Elements Exists");
	}
}
