import java.util.Scanner;
public class Test
{
	public static void main(String[] args)
	{
		System.out.println("Enter the Size of the Array:");
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int [n];
		System.out.printf("Ente %d Elements\n",n);
		for(int i=0;i<arr.length;i++)
		  arr[i]=sc.nextInt();
		  System.out.println(secLargestElement(arr));
	}
	public static int secLargestElement(int arr[])
	{
	    int maxIndex=0;
	    int secMaxIndex=-1;
	    for(int i=1;i<arr.length;i++)
	         if(arr[i]>arr[maxIndex])
	         {
	             secMaxIndex=maxIndex;
	             maxIndex=i;
	         }
             else if((arr[i]<arr[maxIndex]) && (arr[i]>arr[secMaxIndex]))
                 secMaxIndex=i;
	    return arr[secMaxIndex];      
	}
}
