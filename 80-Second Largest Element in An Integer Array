import java.util.*;
public class Test
{
	public static void main(String[] args)
	{
		System.out.println("Enter the Size of the Array:");
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int [n];
		System.out.printf("Ente %d Elements\n",n);
		for(int i=0;i<arr.length;i++)
		  arr[i]=sc.nextInt();
		  sc.close();
		  System.out.println(secLargestElement(arr));
		  System.out.println(secLargestElement1(arr));
	}
	public static int secLargestElement(int arr[])
	{
	    int max=Integer. MIN_VALUE;
	    int secMax=Integer. MIN_VALUE+1;
	    for(int i=0;i<arr.length;i++)
	         if(arr[i]>max)
	         {
	             secMax=max;
	             max=arr[i];
	         }
             else if((arr[i]<max) && (arr[i]>secMax))
                 secMax=arr[i];
	    if(secMax==(Integer. MIN_VALUE+1))
	    	return -1;
	    return secMax;      
	}
	
	public static int secLargestElement1(int arr[])
	{
	    if((arr.length)<2)
	      return -1;
	    Arrays.sort(arr);
	    return arr[arr.length-2];      
	}
}
