import java.util.Scanner;
import java.lang.Math;
public class Main 
{
     public static void main (String[]args) 
     {
    
      Scanner sc = new Scanner (System.in); 
      long n = sc.nextLong();
      System.out.println(binaryToDecimal(n,0));
     } 
 
     public static long binaryToDecimal(long bin,int count) 
     {
         if(bin==0)
           return 0;
         else
            return (bin%10)*(int)Math.pow(2,count)+binaryToDecimal(bin/10,count+1);
     }
}
