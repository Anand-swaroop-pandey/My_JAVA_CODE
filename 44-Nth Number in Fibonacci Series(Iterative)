import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		System.out.println(fibonacci(n));
	}
	
	public static int fibonacci(int n)
	{
	   int a=-1;
	   int b=1;
	   int c=0;
	   for(int i=1;i<=n;i++)
	     {
	         c=a+b; 
	         a=b;
	         b=c;
	     }
	   return c;
	}
}
