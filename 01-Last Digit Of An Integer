import java.util.Scanner;
class Test {
    public static void main(String[] args) {
         Scanner sc=new Scanner(System.in);
         int n=sc.nextInt();
         System.out.println(lastDigit(n));
    }
    public static int lastDigit(int num)
    {
       if(num<0)
         num=Math.abs(num);
         
       return (num%10);
    }
}
