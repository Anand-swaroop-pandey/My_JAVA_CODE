//Bruteforce Approach
import java.util.*;
public class Test
{
     public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       System.out.println("Enter the Size of the Array");
       int n=sc.nextInt();
         if(n>0)
         {
            int[] arr=new int[n];
           System.out.printf("Enter %d Elements of Array:\n",n);
           for(int i=0;i<n;i++)
              arr[i]=sc.nextInt();
           findElements(arr,n);
         }
         else
            System.out.println("Invalid Choice");
	}
    
 public static void findElements(int[] arr,int n)
 {
     if(n<3)
     {
        System.out.println("No Elements in array which have at-least two greater elements than themselves");
        return;
     }  
     
     for(int i=0;i<n;i++)
     {
	 int count=0; 
	 for(int j=0;j<n;j++)
	    if(arr[j]>arr[i])
            {
              count++;
	      if(count==2)
	         break;
	    }
            if(count==2)
              System.out.printf(arr[i]+" ");
      }
   }
}

//Optimal Approach
import java.util.*;
public class Test
{
     public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       System.out.println("Enter the Size of the Array");
       int n=sc.nextInt();
         if(n>0)
         {
            int[] arr=new int[n];
           System.out.printf("Enter %d Elements of Array:\n",n);
           for(int i=0;i<n;i++)
              arr[i]=sc.nextInt();
           findElements(arr,n);
         }
         else
            System.out.println("Invalid Choice");
	}
    
        public static void findElements(int[] arr,int n)
	{
	    if(n<3)
            {
              System.out.println("No Elements in array which have at-least two greater elements than themselves");
              return;
            } 
	  Arrays.sort(arr);
	  for(int i=0;i<(n-2);i++)
	    System.out.printf(arr[i]+" ");
	}
}
