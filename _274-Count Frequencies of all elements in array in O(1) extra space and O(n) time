//Bruteforce Approach

import java.util.Scanner;
public class Test
{
	public static void main(String[] args) {
	   Scanner sc=new Scanner(System.in);
	   System.out.println("Enter the size of the array");
	   int n=sc.nextInt();
	   if(n>0)
	   {
	     int[] arr=new int[n];
	     System.out.printf("Enter %d elements of the array\n",n);
	     for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
	     printFrequency(arr);
	   }
	   else
	     System.out.println("Invalid Choice");
	}
	
	public static void printFrequency(int [] arr)
	{
	     System.out.println("Frequency Of Elements are:");
         int[] freqArr=new int[arr.length];
         for(int i=0;i<arr.length;i++)
            freqArr[arr[i]]++;
            
         for(int i=0;i<arr.length;i++)
             System.out.println(i+"=>"+freqArr[i]);  
	}
}

//Optimal Approach

import java.util.Scanner;
public class Test
{
	public static void main(String[] args) {
	   Scanner sc=new Scanner(System.in);
	   System.out.println("Enter the size of the array");
	   int n=sc.nextInt();
	   if(n>0)
	   {
	     int[] arr=new int[n];
	     System.out.printf("Enter %d elements of the array\n",n);
	     for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
	     printFrequency(arr);
	   }
	   else
	     System.out.println("Invalid Choice");
	}
	
	public static void printFrequency(int [] arr)
	{
	    int n=arr.length;
	    for(int i=0;i<n;i++)
	    {
	        int OrgValAt_ithIndex=arr[i]%n;
	        arr[OrgValAt_ithIndex]+=n;
	    }
	    System.out.println("Frequency Of Elements are:");
	    for(int i=0;i<n;i++)
	    {
	       int countOf_i=(arr[i]/n);
	       System.out.println(i+"=>"+countOf_i);
	    }
	}
}
