//Square root of perfect squares are always whole numbers.

//Bruteforce Approach
import java.util.Scanner;
import java.lang.Math;
public class Test
{
	public static void main(String[] args) 
	{
	    Scanner sc=new Scanner(System.in);
	    int num=sc.nextInt();
	    if(isPerfectSquare(num))
	       System.out.println("is a perfectSquareNumber");
	    else
	       System.out.println("is not a perfectSquareNumber");
	}
	
	public static boolean isPerfectSquare(int num)
	{
            double sqrt=Math.sqrt(num);
            double floor=Math.floor(sqrt);
	    if(sqrt==floor)
	      return true;
	    else
	      return false;   
        }
}

//ALITER Implementation

import java.util.Scanner;
import java.lang.Math;
public class Test
{
	public static void main(String[] args) 
	{
	    Scanner sc=new Scanner(System.in);
	    int num=sc.nextInt();
	    if(isPerfectSquare(num))
	       System.out.println("is a perfectSquareNumber");
	    else
	       System.out.println("is not a perfectSquareNumber");
	}
	
	public static boolean isPerfectSquare(int num)
	{
           double sqrt=Math.sqrt(num);
           double ceil=Math.ceil(sqrt);
	   
	   if(sqrt==ceil)
	     return true;
	   else
	     return false;   
        }
}

//Optimal Approach

import java.util.Scanner;
import java.lang.Math;
public class Test
{
	 public static void main(String[] args) 
	 {
	    Scanner sc=new Scanner(System.in);
	    int num=sc.nextInt();
	    if(isPerfectSquare(num))
	       System.out.println("is a perfectSquareNumber");
	    else
	       System.out.println("is not a perfectSquareNumber");
	  }
	
	  public static boolean isPerfectSquare(int num)
	  {
             return  mySqrt(num);
          }
           
	  public static boolean mySqrt(int x)
          {
            if(x==1)
              return true; //1 is a perfect Square Number
        
            return BinarySearch(x);  
          }
   
          public static boolean BinarySearch(int x)
          {
              int low =1,high=x-1;
              while(low<=high)
              {
                int mid=low+(high-low)/2;
		long sqr=mid*mid;
                if(x==sqr)
                  return true; //x is a perfect Square Number
                else if(x>sqr)
                  low=mid+1;
                else//if(x<sqr)
                  high=mid-1;
              }
             return false; //x is not a perfect Square Number
          }
}
