import java.util.Scanner;
class Test {
    public static void main(String[] args) {
         Scanner sc=new Scanner(System.in);
         int n=sc.nextInt();
         System.out.println(reverse(n));
    }
    public static int reverse(int num)
    {
        int rev=0;
        while(num!=0)
        {
          int last=(num%10);
           rev=rev*10+last; 
           num=num/10;
        }
        return rev;
    }
}
