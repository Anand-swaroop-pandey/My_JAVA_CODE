//Bruteforce Approach
import java.util.Scanner;
public class Test
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.println("Enter the Size of the Array");
	    int n=sc.nextInt();
	    int[] arr=new int[n];
	     System.out.printf("Enter %d Elements\n",n);
	    for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
      System.out.println("Elements occurring Twice is: ");
      findTwoRepeated(arr);
	}
	
  public static void findTwoRepeated(int arr[])
	{
  	    int n=arr.length;
  	    int count=0;
	    for(int i=0;i<n;i++)
	        for(int j=i+1;j<n;j++)
	            if(arr[i]==arr[j])
	            {
	                count++;
	                System.out.println(arr[i]+" ");
	                if(count==2)
	                  return;
	            }
                    
	}
}

//Improved Bruteforce Approach
import java.util.*;
public class Test
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.println("Enter the Size of the Array");
	    int n=sc.nextInt();
	    int[] arr=new int[n];
	     System.out.printf("Enter %d Elements\n",n);
	    for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
      System.out.println("Elements occurring Twice is: ");
      findTwoRepeated(arr);
	}
	
	public static void findTwoRepeated(int arr[])
	{
      int n=arr.length;
  	  Arrays.sort(arr);
  	  int count=0;
	    for(int i=0;i<n-1;i++)
	        if(arr[i]==arr[i+1])
	        {
	            count++;
	            System.out.println(arr[i]+" ");
	            i++;
	            if(count==2)
	              break;
	        }
	}
}


//Improved Approach
import java.util.Scanner;
public class Test
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.println("Enter the Size of the Array");
	    int n=sc.nextInt();
	    int[] arr=new int[n];
	     System.out.printf("Enter %d Elements\n",n);
	    for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
      System.out.println("Elements occurring Twice is: ");
      findTwoRepeated(arr);
	}
	
  public static void findTwoRepeated(int arr[])
	{
	    int res=-1;
	    int[] freqArr=new int[arr.length];
	    countFrequency(arr,freqArr);
	    int count=0;
	    for(int i=0;i<arr.length;i++)
	       if(freqArr[i]==2)
	       {
	           count++;
	           System.out.println(arr[i]+" ");
	           if(count==2)
	             break;
	       } 
	}
	
	public static void countFrequency(int[] arr,int[] freqArr)
	{
	       int n=arr.length;
	       int count;   
           for(int i=0;i<n;i++)
           {
             if(freqArr[i]==-1)
               continue;
             count=1;
             for(int j=i+1;j<n;j++)
                if(arr[i]==arr[j])
                {
                   freqArr[j]=-1;
                   count++;
                }
            freqArr[i]=count; 
           }
	}
}


//Better Approach

import java.util.Scanner;
public class Test
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.println("Enter the Size of the Array");
	    int n=sc.nextInt();
	    int[] arr=new int[n];
	     System.out.printf("Enter %d Elements\n",n);
	    for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
        System.out.println("Elements occurring Twice is: ");
        findTwoRepeated(arr);
	}
	
  public static void findTwoRepeated(int arr[])
	{
	  int n=arr.length;
      int sum=((n-2)*(n-2+1))/2;//Sum of First (n-2) Natural Numbers.
      int prod=factorial(n-2);//Product of First (n-2) Natural Numbers.
      int arrSum=0,arrProd=1;
      for(int i=0;i<n;i++)
      {
        arrSum+=arr[i];
        arrProd*=arr[i];
      }
      int sumOfTwoRepeating=arrSum-sum;//R1+R2=sumOfTwoRepeating
      int prodOfTwoRepeating=arrProd/prod;//R1*R2=prodOfTwoRepeating
      int differenceOfTwoRepeating=(int)Math.sqrt((sumOfTwoRepeating*sumOfTwoRepeating)-(4*prodOfTwoRepeating));//R1-R2=differenceOfTwoRepeating //(X-Y)^2=((X+Y)^2 - 4*X*Y)
      int R1=(sumOfTwoRepeating+differenceOfTwoRepeating)/2;//(R1+R2)+(R1-R2)=2R1 => R1=((R1+R2)+(R1-R2))/2
      int R2=sumOfTwoRepeating-R1;//R2=(R1+R2)-R1
      System.out.println(R1+" "+R2);
	}
	
	public static int factorial(int num)
	{
	    if(num==0)
	      return 1;
	    return (num*factorial(num-1));
	}
}
 
 
//Optimal Approach
import java.util.Scanner;
public class Test
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.println("Enter the Size of the Array");
	    int n=sc.nextInt();
	    int[] arr=new int[n];
	     System.out.printf("Enter %d Elements\n",n);
	    for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
        System.out.println("Elements occurring Twice is: ");
        findTwoRepeated(arr);
	}
	
    public static void findTwoRepeated(int arr[])
	 {
      int n=arr.length;
      int[] count=new int[n-1];//array size is (n+2) and array element are in the range of 1 to N(both inclusive)
      for(int i=0;i<n;i++)
        count[arr[i]]++;
        
        int cnt=0;
       for(int i=1;i<(n-1);i++)
          if(count[i]==2)
          {
             cnt++;
	         System.out.println(arr[i]+" ");
	         if(cnt==2)
	            break;
          }
	 }
}


//Further Optimised Approach
import java.util.Scanner;
public class Test
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.println("Enter the Size of the Array");
	    int n=sc.nextInt();
	    int[] arr=new int[n];
	     System.out.printf("Enter %d Elements\n",n);
	    for(int i=0;i<n;i++)
	       arr[i]=sc.nextInt();
        System.out.println("Elements occurring Twice is: ");
        findTwoRepeated(arr);
	}
	
    public static void findTwoRepeated(int arr[])
	 {
       int n=arr.length;
       int count=0;
       for(int i=0;i<n;i++)
       {
           int element=arr[Math.abs(arr[i])]; 
           if(element <0)
           {
              count++;
              System.out.println(Math.abs(arr[i])+" ");
              if(count==2)
	             break;
           }
           arr[Math.abs(arr[i])]=-arr[Math.abs(arr[i])]; 
       }
	 }
}
