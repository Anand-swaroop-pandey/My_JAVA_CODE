import java.util.Scanner;
public class Test
{
  public static void main (String[]args)
  {

    Scanner sc = new Scanner (System.in);
    int num= sc.nextInt();
    if(isEven(num))
       System.out.println ("is EVEN");
    else
       System.out.println ("is ODD");
  }

  public static boolean isEven(int num)
  {
    if((num&1)==0)
      return true;
    else 
      return false;
  }
}
