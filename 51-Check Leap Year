import java.util.Scanner;
public class Test
{
	public static void main(String[] args)
	{
	    Scanner sc=new Scanner(System.in);
	    int year=sc.nextInt();
	    System.out.println(leapYear(year));
	}
    
	public static boolean leapYear(int year)
	{
           if (year % 400 == 0) 
            return true;
           else if (year % 100 == 0)
            return false;
           else if (year % 4 == 0)
            return true;
           else
            return false;
	}
	
}
